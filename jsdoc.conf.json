{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./src"],
    "includePattern": "\\.(jsx|js|ts|tsx)$"
  },
  "plugins": ["plugins/markdown", "better-docs/component", "better-docs/typescript", "better-docs/category"],
  "opts": {
    "encoding": "utf8",
    "destination": "docs",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/better-docs",
    "readme": "README.md"
  },
  "templates": {
    "better-docs": {
          "name": "Sportify-FR documentation"
    },
    "search": true
}
}